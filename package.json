{
  "name": "encode-decode-suite",
  "version": "1.0.0",
  "description": "Encodes and decodes some of the web's most commonly used formats",
  "main": "index.js",
  "scripts": {
    "start": "http-server ./app | npm run watch",
    "test": "echo \"Error: no test specified\" && exit 1",
    "bundle": "browserify ./src/scripts/startup.js -o ./app/scripts/bundle.js -v",
    "build-js": "browserify -t babelify -g uglify-js-harmony -c ./src/scripts/startup.js -o ./app/scripts/bundle.js",
    "jss": "browserify -t babelify -g uglify-js-harmony ./src/scripts/startup.js > ./app/scripts/bundle.js",
    "watch": "watchify ./src/scripts/startup.js -d -o ./app/scripts/bundle.js -v",
    "build": "npm run build-js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/unkani/encode-decode-suite.git"
  },
  "keywords": [
    "encode",
    "decode",
    "codec",
    "base64",
    "jwt",
    "hex",
    "html",
    "binary"
  ],
  "author": "Neil Wang",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/unkani/encode-decode-suite/issues"
  },
  "homepage": "https://github.com/unkani/encode-decode-suite#readme",
  "devDependencies": {
    "babel": "^6.5.2",
    "babelify": "^7.3.0",
    "browserify": "^13.1.1",
    "exorcist": "^0.4.0",
    "http-server": "^0.9.0",
    "uglify-js-harmony": "^2.6.2",
    "watchify": "^3.8.0"
  },
  "dependencies": {
    "jsonwebtoken": "^7.2.1",
    "lzutf8": "^0.3.5"
  }
}
